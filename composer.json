{
  "name": "honeydt/FATheme",
  "description": "Simple theme for Codeigntier 4",
  "license": "MIT",
  "homepage": "https://github.com/honeydt/FATheme",
  "keywords": [
    "theme",
    "codeigniter"
  ],
  "authors": [
    {
      "name": "Nguyen Duy Toai",
      "email": "ihoney_dt@yahoo.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "repositories": [{  
        "type": "package",
        "package": {
            "name": "honeydt/FATheme", //give package name to anything, must be unique
            "version": "1.0",
            "source": {
                "url": "https://github.com/honeydt/FATheme.git", //git url
                "type": "git",
                "reference": "master" //git branch-name
            }
        }
  }],
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "HoneyDT\\FATheme\\": "src"
    }
  }
}
